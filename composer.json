{
    "name": "bramdevries/oauth2-slack",
    "description": "Slack OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Devries",
            "email": "bram@madewithlove.be",
            "homepage": "https://github.com/bramdevries"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "slack"
    ],
    "require": {
        "php": "~8.1",
        "league/oauth2-client": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0",
        "mockery/mockery": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Bramdevries\\Oauth\\Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bramdevries\\Oauth\\Client\\": "spec"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
